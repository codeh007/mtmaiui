{
  "name": "mtmaiui",
  "private": false,
  "version": "0.5.1",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "files": ["dist", "src"],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite --port=6111",
    "dev:next": "next dev",
    "dev:ssr": "node server",
    "build": "vite build --emptyOutDir=false",
    "build:vite": "vite build",
    "build:types": "vite build --types",
    "build:server": "vite build --ssr src/entry-server.tsx",
    "serve": "vite preview",
    "start": "vite",
    "page:deploy": "bun run ./src/cli/main.ts dp_mtmaiui"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*.ts",
        "src/*.tsx",
        "src/*/index.ts",
        "src/*/index.tsx",
        "dist/*.d.ts",
        "dist/*",
        "dist/*/index"
      ]
    }
  },
  "exports": {
    ".": {
      "types": ["./src/index.ts", "./dist/index.js", "./dist/index.d.ts"],
      "import": ["./src/index.ts", "./dist/index.js", "./dist/*"]
    },
    "./components/*/*": {
      "types": "./dist/components/*/*.d.ts",
      "import": "./dist/components/*/*.js",
      "require": "./dist/components/*/*.js"
    },
    "./*": {
      "types": [
        "./src/*.tsx",
        "./src/*.ts",
        "./src/*/index.ts",
        "./src/*.tsx",
        "./src/*/index.tsx",
        "./dist/*.d.ts",
        "./dist/*/index.d.ts"
      ],
      "import": [
        "./src/*",
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*/index.ts",
        "./src/*/index.tsx",
        "./dist/*/index.js",
        "./dist/*.js"
      ]
    }
  },
  "dependencies": {
    "tailwind-scrollbar": "^3.1.0",
    "tailwind-scrollbar-hide": "^1.1.7",
    "@codemirror/lang-sql": "^6.8.0",
    "@tanstack/react-query": "^5.62.14",
    "@tanstack/react-query-devtools": "^5.62.14",
    "@tanstack/react-query-next-experimental": "^5.62.14",
    "@tanstack/react-router": "^1.93.0",
    "@tanstack/router-devtools": "^1.93.0",
    "@tanstack/router-plugin": "^1.93.0",
    "@tanstack/start": "^1.93.0",
    "get-port": "^7.1.0",
    "mtmaiapi": "*",
    "mtxuilib": "*",
    "next": "^15.1.3",
    "react": "^19",
    "react-dom": "^19",
    "redaxios": "^0.5.1",
    "superjson": "^2.2.2",
    "unenv": "^1.10.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/generator": "^7.26.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/express": "^4.17.21",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "compression": "^1.7.5",
    "express": "^4.21.2",
    "isbot": "^5.1.17",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.49",
    "serve-static": "^1.16.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite-plugin-ssr": "^0.4.142"
  }
}
